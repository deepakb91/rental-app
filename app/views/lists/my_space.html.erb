<div id="table_container_myspace">
	<table class="table table-striped table-condensed" id="mytable">
		<tr>
			<th>TITLE</th>
			<th>ROOM-TYPE</th>
			<th>CITY</th>
			<th>STATE</th>
			<th>COUNTRY</th>
			<th colspan="3"></th>
		</tr>
		<% @lists.each do |list|%>
			<%if list.user_id==current_user.id%>
				<tr>
					<td><%= list.title%></td>
					<td><%= list.room_type%></td>
					<td><%= list.city%></td>
					<td><%= list.state%></td>
					<td><%= list.country%></td>
					<td><%= link_to 'View Space',view_myspace_path(:id=>list.id), :class=>"btn btn-success btn-small"%></td>
					<td><%= link_to 'Edit Space',edit_list_path(list), :class=>"btn btn-warning btn-small"%></td>
					<td><%= link_to 'Remove Space',list, :class=>"btn btn-danger btn-small", :confirm => 'Are you sure?', :method => :delete %></td>
				</tr>
			<%end%>
		<%end%>
	</table>
</div>